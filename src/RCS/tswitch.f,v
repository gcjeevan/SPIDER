      DATA MENU(61),  MENUL(61),  IW(61)  /'OR','  ',3/
      DATA MENU(62),  MENUL(62),  IW(62)  /'PA','PATCH',2/
      DATA MENU(153), MENUL(153), IW(153) /'PB','PDB',15/
